    "pg-hstore": "^2.3.4",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.1"
  }
